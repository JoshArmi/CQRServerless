        "eslint-plugin-prettier": "^3.1.4",
        "jest": "25.0.0",
        "prettier": "^2.0.5",
        "ts-jest": "^25.3.1",
        "ts-loader": "^8.0.2",
        "ts-node": "^8.1.0",
