{
    "type": "module",
    "devDependencies": {
        "@types/cucumber": "^6.0.1",
        "@types/jest": "^26.0.9",
        "@types/node": "^14.0.27",
        "@types/webpack": "^4.41.21",
        "@typescript-eslint/eslint-plugin": "^3.9.0",
